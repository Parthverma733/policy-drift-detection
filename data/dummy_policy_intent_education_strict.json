{
  "scheme_name": "Strict Education Quality Assurance Policy",
  "target_groups": [
    {
      "name": "priority_districts",
      "criteria": {
        "literacy_rate": {"max": 65.0},
        "population": {"min": 50000}
      },
      "allocation_priority": 1
    },
    {
      "name": "standard_districts",
      "criteria": {
        "literacy_rate": {"min": 65.0, "max": 80.0}
      },
      "allocation_priority": 2
    }
  ],
  "constraints": [
    {
      "type": "minimum_coverage",
      "metric": "coverage_percentage",
      "threshold": 90.0,
      "applies_to": "priority_districts"
    },
    {
      "type": "minimum_coverage",
      "metric": "coverage_percentage",
      "threshold": 85.0,
      "applies_to": "standard_districts"
    },
    {
      "type": "temporal_consistency",
      "metric": "monthly_variance",
      "threshold": 8.0,
      "applies_to": "all"
    },
    {
      "type": "resource_allocation",
      "metric": "fund_utilization",
      "threshold": 92.0,
      "applies_to": "priority_districts"
    },
    {
      "type": "resource_allocation",
      "metric": "fund_utilization",
      "threshold": 88.0,
      "applies_to": "standard_districts"
    }
  ],
  "target_metrics": {
    "coverage_percentage": {"min": 85.0},
    "fund_utilization": {"min": 85.0},
    "monthly_variance": {"max": 10.0}
  }
}
